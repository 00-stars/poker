{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "globals": {
    "window": true,
    "alert": true,
    "WebSocket": true
  },
  "rules": {
    "prefer-const": 0,
    "comma-dangle": 0,
    "no-plusplus": 0,
    "no-alert": 0,
    "class-methods-use-this": 0,
    "space-before-function-paren": [1, "never"],
    "no-param-reassign": [2, { "props": false }]
  }
}
